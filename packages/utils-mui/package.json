{
  "name": "@sheetxl/utils-mui",
  "version": "0.0.0",
  "description": "Utilities MUI - Utilities for MUI Components needed for all SheetXL components.",
  "homepage": "https://sheetxl.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sheetxl/sheetxl.git"
  },
  "bugs": {
    "url": "https://github.com/sheetxl/sheetxl/issues"
  },
  "license": "SEE LICENSE IN <LICENSE>",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./build/types/index.d.ts",
      "import": {
        "node": "./build/node/esm/index.mjs",
        "browser": "./build/browser/esm/index.mjs",
        "default": "./build/browser/esm/index.mjs"
      }
    }
  },
  "scripts": {
    "build:node": "cross-env BUILD_ENV=node vite build",
    "build:browser": "vite build",
    "build": "pnpm run build:node && pnpm run build:browser",
    "build:tsdocs:json": "typedoc --json ../../build/docs/utils-mui.json --excludeInternal --tsconfig ./tsconfig.build.json",
    "clean": "rimraf ./build",
    "clean:all": "git clean -fdX .",
    "release:package": "cd build && pnpm publish --no-git-checks --access public",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@sheetxl/utils": "workspace:*",
    "@sheetxl/utils-react": "workspace:*",
    "clsx": "^2.1.1",
    "detect-it": "^4.0.1",
    "focus-trap-react": "^10.1.4",
    "mui-modal-provider": "2.4.6",
    "react-draggable": "^4.5.0",
    "react-hook-form": "^7.64.0",
    "react-merge-refs": "^3.0.2",
    "react-use": "^17.6.0",
    "tiny-invariant": "^1.3.3"
  },
  "peerDependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.0",
    "@mui/styled-engine": "^7.0.0",
    "@mui/system": "^7.0.0",
    "@mui/utils": "^7.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0"
  },
  "sideEffects": [
    "*.css",
    "**/*.css"
  ],
  "keywords": [
    "sheetxl",
    "spreadsheet",
    "javascript",
    "typescript",
    "data grid",
    "excel",
    "xlsx",
    "utilities",
    "mui",
    "react",
    "material-ui",
    "material",
    "ui"
  ]
}
