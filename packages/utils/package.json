{
  "name": "@sheetxl/utils",
  "version": "0.0.0",
  "description": "Utils - Utilities used by most SheetXL components.",
  "homepage": "https://sheetxl.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sheetxl/sheetxl.git"
  },
  "bugs": {
    "url": "https://github.com/sheetxl/sheetxl/issues"
  },
  "license": "SEE LICENSE IN <LICENSE>",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./build/types/index.d.ts",
      "import": {
        "node": "./build/node/esm/index.mjs",
        "browser": "./build/browser/esm/index.mjs",
        "default": "./build/browser/esm/index.mjs"
      }
    }
  },
  "scripts": {
    "build:node": "cross-env BUILD_ENV=node vite build",
    "build:browser": "vite build",
    "build": "pnpm run build:node && pnpm run build:browser",
    "build:tsdocs:json": "typedoc --json ../../build/docs/utils.json --excludeInternal --tsconfig ./tsconfig.build.json",
    "clean": "rimraf ./build",
    "clean:all": "git clean -fdX .",
    "release:package": "cd build && pnpm publish --no-git-checks --access public",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "sideEffects": false,
  "keywords": [
    "sheetxl",
    "spreadsheet",
    "javascript",
    "typescript",
    "data grid",
    "excel",
    "xlsx",
    "utilities"
  ],
  "gitHead": "148c048e9dc00598c40c8ea2012d2951c73e7e12"
}
