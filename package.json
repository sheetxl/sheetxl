{
  "name": "@sheetxl/monorepo",
  "description": "SheetXL monorepo.",
  "version": "0.0.0",
  "license": "SEE LICENSE IN <LICENSE>",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "echo Mono repo does not have a dev script; try 'dev:studio', 'dev:docsite', or 'dev:storybook'",
    "start": "echo Mono repo does not have a start script; try 'dev:studio', 'dev:docsite', or 'dev:storybook'",
    "dev:studio-mui": "pnpm --filter ./examples/studio-mui dev",
    "dev:docsite": "pnpm --filter ./docsite dev",
    "dev:storybook": "pnpm --filter ./examples/storybook dev",
    "build": "pnpm -r --workspace-concurrency=1 --bail --filter=\"./packages/*\" build",
    "build:docsite": "pnpm --filter ./docsite build",
    "build:tsdocs:json": "pnpm --filter=\"./packages/*\" build:tsdocs:json",
    "build:tsdocs:merge": "typedoc -favicon ./static/img/favicon.ico --excludeInternal --entryPointStrategy merge \"./build/docs/*.json\"",
    "build:tsdocs": "pnpm run build:tsdocs:json && pnpm run build:tsdocs:merge",
    "build:studio-mui": "pnpm -filter ./examples/studio-mui build",
    "build:storybook": "pnpm --filter ./examples/storybook build",
    "clean": "pnpm -r clean",
    "clean:build": "rimraf ./build && pnpm -r clean:build",
    "clean:tsdocs": "rimraf ./build/docs",
    "clean:all": "git clean -fdX .",
    "release:public-repo": "rimraf ../deploy-public && node ./config/publishPublicRepo.ts",
    "lint": "eslint --cache --quiet .",
    "lint:full": "eslint --cache --max-warnings 10000 .",
    "format": "prettier --ignore-path .eslintignore --write .",
    "format:check": "prettier --ignore-path .eslintignore --check .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:node": "vitest --config vitest.node.config.ts",
    "test:jsdom": "vitest --config vitest.jsdom.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:coverage:node": "vitest run --coverage --config vitest.node.config.ts",
    "test:coverage:jsdom": "vitest run --coverage --config vitest.jsdom.config.ts",
    "preview:tsdocs": "npx http-server ./build/docs -p 3200 -c-1 --cors",
    "preview:docsite": "pnpm --filter ./docsite preview",
    "preview:studio": "pnpm --filter ./examples/studio-mui preview",
    "preview:storybook": "pnpm --filter ./examples/storybook preview",
    "sheetxl": "pnpm --filter ./packages/cli sheetxl",
    "publish": "echo 'Use release",
    "release:packages": "pnpm --filter=\"./packages/*\" release:package",
    "release": "pnpm semantic-release --extends ./config/release.config.ts",
    "release:no-ci": "cross-env GITHUB_TOKEN=fake pnpm semantic-release --extends ./config/release.config.ts --no-ci",
    "release:test": "cross-env GITHUB_TOKEN=fake pnpm semantic-release --extends ./config/release.config.ts --no-ci --dry-run"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@csstools/css-parser-algorithms": "^3.0.5",
    "@csstools/css-tokenizer": "^3.0.4",
    "@eslint/js": "^9.36.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "@rollup/pluginutils": "^5.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@svgr/rollup": "^8.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/ui": "3.2.4",
    "auto-changelog": "^2.5.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "chalk": "^5.6.2",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.0.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "glob": "^11.0.1",
    "http-server": "^14.1.1",
    "javascript-obfuscator": "^4.1.1",
    "jsdom": "^27.0.0",
    "jsonc-parser": "^3.3.1",
    "postcss": "^8.4.38",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "prompt-confirm": "^2.0.4",
    "rimraf": "^6.0.1",
    "rollup": "4.52.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-license": "^3.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-visualizer": "^6.0.3",
    "semantic-release": "^24.2.9",
    "semgrep": "^0.0.1",
    "semver": "^7.3.5",
    "terser": "^5.44.0",
    "typedoc": "^0.28.13",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "packageManager": "pnpm@10.12.3",
  "pnpm": {
    "overrides": {}
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20 <21 || >=22",
    "pnpm": ">=10"
  },
  "engineStrict": true
}
